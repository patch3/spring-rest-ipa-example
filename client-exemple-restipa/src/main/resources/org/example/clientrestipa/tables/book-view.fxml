<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21"
            fx:controller="org.example.clientrestipa.controllers.tables.BookController">
    <TableView fx:id="table" layoutX="2.0" layoutY="51.0" prefHeight="348.0" prefWidth="575.0"
               AnchorPane.bottomAnchor="1.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0"
               AnchorPane.topAnchor="51.0">
    </TableView>
    <HBox alignment="CENTER" layoutY="1.0" prefHeight="51.0" prefWidth="600.0" spacing="15.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <children>
            <Label text="Book: ">
                <font>
                    <Font name="System Bold" size="16.0"/>
                </font>
            </Label>
            <TextField fx:id="nameField" promptText="Name"/>
            <Button fx:id="addButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="78.0" text="ADD">
                <font>
                    <Font size="11.0"/>
                </font>
            </Button>
        </children>
    </HBox>
</AnchorPane>
